    Songkick.EventBus.bind('app:initialize', function() {
      $('.search-text-input').on('focusin', function(){
        $(this).addClass('active');
        $(this).attr('placeholder', "Find concerts for any artist or city");
      }).on('focusout', function(){
        $(this).removeClass('active');
        $(this).attr('placeholder', "Find concerts");
      });
    });

