  <script type="text/javascript">
    Songkick.EventBus.bind('app:initialize', function(config) {
      googletag = window.googletag || {};
      googletag.cmd = window.googletag.cmd || [];
      var targetingTags = {"artist":"Blind Guardian","tour_status":"on_tour"};
      try {
        if (typeof shouldHideStickyFooter === 'undefined') {
          new Songkick.Component.Ads(freestar, googletag, targetingTags, config.events, false);
        } else {
          new Songkick.Component.Ads(freestar, googletag, targetingTags, config.events, shouldHideStickyFooter);
        }
      }
      catch(err) {
        console.log('Ads Error:');
        console.log(err);
      }
    });
  </script>


