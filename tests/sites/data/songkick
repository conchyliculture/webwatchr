<script>
$(document).ready(function() {
    updateButtonText();

    $(window).resize(function() {
        updateButtonText();
    });
});

function updateButtonText() {
    var windowWidth = $(window).width();
    var buttonText = windowWidth < 1100 ? "Search" : "Search for events or artists";
    $("#searchButtonText").text(buttonText);
}
</script>
